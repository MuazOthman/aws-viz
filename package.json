    "@types/html-to-text": "^9.0.0",
    "@types/jest": "^29.2.5",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^18.11.18",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.8.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "fast-xml-parser": "^4.2.4",
    "html-to-text": "^9.0.3",
    "lodash.isequal": "^4.5.0",
    "lodash.uniq": "^4.5.0",
    "pako": "^2.1.0",
    "yaml-cfn": "^0.3.2"
  }
